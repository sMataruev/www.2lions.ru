mixin title(title, urll, addtionalClass)
	.title(style=`background-image: url('../img/bg/${urll}')` class='addtionalClass')= title

mixin svg(svg_url, svg_id)
	if svg_id
		svg()&attributes(attributes)
			use(xlink:href= svg_url + '#' + svg_id)
	else
		svg()&attributes(attributes)
			use(xlink:href= svg_url)