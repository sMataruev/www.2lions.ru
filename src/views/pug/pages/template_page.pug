						li.content_footer_block_printing_item
							a(href='#').content_footer_block_printing_item__poster Афиши, плакаты
						li.content_footer_block_printing_item
							a(href='#').content_footer_block_printing_item__stationary Канцелярия
						li.content_footer_block_printing_item
							a(href='#').content_footer_block_printing_item__menu Меню, ценники
						li.content_footer_block_printing_item
							a(href='#').content_footer_block_printing_item__stickers Наклейки
				.content_footer_block_advert
					ul.content_footer_block_advert_items
						li.content_footer_block_advert_item
							a(href='#').content_footer_block_advert_item__advertising РЕКЛАМА
						li.content_footer_block_advert_item
							a(href='#').content_footer_block_advert_item__boards Наружная
						li.content_footer_block_advert_item
							a(href='#').content_footer_block_advert_item__audio_video Аудио и видео
