				.content_footer_block_inet
					ul.content_footer_block_inet_items
						li.content_footer_block_inet_item
							a(href='#').content_footer_block_inet_item__services ИНТЕРНЕТ УСЛУГИ
						li.content_footer_block_inet_item
							a(href='#').content_footer_block_inet_item__app_dev Разработка сайтов
								br
								| и приложений
						li.content_footer_block_inet_item
							a(href='#').content_footer_block_inet_item__content_creation Создание контента
						li.content_footer_block_inet_item
							a(href='#').content_footer_block_inet_item__call_center Организация call-center
